{
  "extends" : [
    "rallycoding",
    "prettier"
  ],
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  },
  "rules" : {
    "global-require" : 0,
    "prettier/prettier": ["error", {"singleQuote": false, "parser": "flow"}]
  }
}
